  "devDependencies": {
    "@types/chai": "^4.0.2",
    "@types/lodash": "^4.14.71",
    "@types/mocha": "^2.2.41",
    "chai": "^4.1.1",
    "mocha": "^3.5.0",
    "rimraf": "^2.6.1",
    "source-map-support": "^0.4.15",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2"
  },
